{
    "name": "jobcerto/nova-add-more",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Jobcerto\\NovaAddMore\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jobcerto\\NovaAddMore\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
